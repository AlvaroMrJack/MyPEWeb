
{# Footer #}
<footer id="contacto" class="page-footer {{ footer_color }} scrollspy">
    <div class="container">  
        <div class="row">
            <div class="col l4 s12">
                <h5 class="white-text">Redes sociales</h5>
                <ul>
                {% for datos in rrss %}
                    <li>
                        <a class="white-text" href="https://{{ datos }}">
                            <i class="small fa {{ datos }} white-text"></i>  {{ datos }}
                        </a>
                    </li>
                {% endfor %}
                </ul>
            </div>
            <div class="col l8 s12">
                <div class="row">
                    <div class="input-field col s6">
                        <i class="mdi-action-account-circle prefix {{ mailicon_color }}-text"></i>
                        <input id="form_name" name="name" type="text" class="validate {{ mailicon_color }}-text">
                        <label for="icon_prefix" class="{{ mailfont_color }}-text">Nombre</label>
                    </div>
                    <div class="input-field col s6">
                        <i class="mdi-communication-email prefix {{ mailicon_color }}-text"></i>
                        <input id="form_email" name="email" type="email" class="validate {{ mailicon_color }}-text">
                        <label for="icon_email" class="{{ mailfont_color }}-text">Correo electr√≥nico</label>
                    </div>
                    <div class="input-field col s12">
                        <i class="mdi-editor-mode-edit prefix {{ mailicon_color }}-text"></i>
                        <textarea id="msg_text" name="message" class="materialize-textarea {{ mailicon_color }}-text"></textarea>
                        <label for="icon_prefix2" class="{{ mailfont_color }}-text">Mensaje</label>
                    </div>
                    <div class="col offset-s7 s5">
                        <button id="sendMail" class="btn waves-effect waves-light red darken-1">Enviar
                            <i class="mdi-content-send right white-text"></i>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="footer-copyright {{ footer_color_end }}">
        <div class="container">
            Made by <a class="white-text" href="http://joashpereira.com">Joash Pereira</a>. Thanks to <a class="white-text" href="http://materializecss.com/">materializecss</a>
        </div>
    </div>
</footer>